{
    "assignees": [
        "@krilling",
        "@joram.stephenson"
    ],
    "baseBranches": [
        "development"
    ],
    "automerge": false,
    "gitLabAutomerge": false,
    "draftPR": true,
    "packageRules": [
      {
        "datasources": [
          "docker"
        ],
        "packageNames": [
          "registry1.dso.mil/ironbank/ironbank-pipelines/pipeline-runner"
        ],
        "separateMinorPatch": true
      }
    ],
    "regexManagers": [
      {
        "fileMatch": [
          "^Dockerfile$"
        ],
        "matchStrings": [
          "BASE_TAG=(?<currentValue>.+?)"
        ],
        "depNameTemplate": "registry1.dso.mil/ironbank/ironbank-pipelines/pipeline-runner",
        "datasourceTemplate": "docker"
      },
      {
        "fileMatch": [
          "^hardening_manifest.yaml$"
        ],
        "matchStrings": [
          "org\\.opencontainers\\.image\\.version:\\s+(\\s|\"|')?(?<currentValue>.+?)-management",
          "tags:\\s+-(\\s|\"|')+(?<currentValue>.+?)(\\s|\"|'|$)+",
          "BASE_TAG:\\s+\"(?<currentValue>.*?)\""
        ],
        "depNameTemplate": "registry1.dso.mil/ironbank/ironbank-pipelines/pipeline-runner",
        "datasourceTemplate": "docker"
      }
    ]
}
